>\n  );\n}\n\nexport default App;\n","/home/abdelrhman/projects/react-task-tracker/src/components/Header.js",[],"/home/abdelrhman/projects/react-task-tracker/src/components/AddTask.js",[],{"ruleId":"36","severity":2,"message":"37","line":53,"column":69,"nodeType":"38","messageId":"39","endLine":53,"endColumn":73},"no-undef","'show' is not defined.","Identifier","undef"]